{
    "name": "getdkan/datastore",
    "description": "A Drupal-independent version of DKAN's datastore.",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "fmizzell",
            "email": "fmizzell.dev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dkan\\Datastore\\": "src/",
            "Dkan\\DatastoreTest\\": "test/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~7.5.0",
        "bugatino/phpcs-git-pre-commit": "dev-master",
        "squizlabs/php_codesniffer": "~3.4.0"
    },
    "scripts": {
        "post-install-cmd": [
            "sh ./vendor/bugatino/phpcs-git-pre-commit/src/setup.sh"
        ],
        "post-update-cmd": [
            "sh ./vendor/bugatino/phpcs-git-pre-commit/src/setup.sh"
        ]
    },
    "require": {
        "getdkan/csv-parser": "~1.1.0",
        "getdkan/locker": "~1.1.0",
        "getdkan/contracts": "~0.1.0",
        "getdkan/procrastinator": "~1.0.4"
    }
}
